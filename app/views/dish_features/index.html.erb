<div class="well">
  <h1>Listing dish_features</h1>
  <br />

  <table class='table table-hover table-bordered'>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th>Store</th>
      <th colspan="3">Action</th>
    </tr>
    <% @dish_features.each do |dish_feature| %>
        <tr>
          <td><%= dish_feature.name %></td>
          <td><%= dish_feature.desc %></td>
          <td><%= dish_feature.store.name %></td>
          <td><%= link_to 'Show', [@store, dish_feature] %></td>
          <td><%= link_to 'Edit', edit_store_dish_feature_path(@store, dish_feature) %></td>
          <td><%= link_to 'Destroy', [@store, dish_feature], method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
  </table>
  <br />

  <%= link_to 'New Dish feature', new_store_dish_feature_path(@store), :class => 'btn btn-primary' %>
</div>
