<h1>Listing dish_choices</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Desc</th>
    <th>Store</th>
    <th>Must</th>
    <th>Checked</th>
    <th>Input type</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @dish_choices.each do |dish_choice| %>
  <tr>
    <td><%= dish_choice.name %></td>
    <td><%= dish_choice.desc %></td>
    <td><%= dish_choice.store %></td>
    <td><%= dish_choice.must %></td>
    <td><%= dish_choice.checked %></td>
    <td><%= dish_choice.input_type %></td>
    <td><%= dish_choice.content %></td>
    <td><%= link_to 'Show', dish_choice %></td>
    <td><%= link_to 'Edit', edit_dish_choice_path(dish_choice) %></td>
    <td><%= link_to 'Destroy', dish_choice, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Dish choice', new_dish_choice_path %>
