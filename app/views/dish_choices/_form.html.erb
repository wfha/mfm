<%= simple_form_for([@store, @dish_choice]) do |f| %>
    <%= f.error_notification %>
    <%= f.input :name %>
    <%= f.input :desc %>
    <%= f.input :must %>
    <%= f.input :input_type %>
    <%= f.input :checked %>
    <%= f.input :content %>
    <%= f.button :submit, 'Submit', class: 'btn btn-primary' %>
<% end %>