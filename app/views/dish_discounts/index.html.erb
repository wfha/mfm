<div class="well">
  <h1>Listing dish_discounts</h1>

  <table class='table table-hover table-bordered'>
    <tr>
      <th>Name</th>
      <th>Desc</th>
      <th>Price</th>
      <th>Store</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>

    <% @dish_discounts.each do |dish_discount| %>
        <tr>
          <td><%= dish_discount.name %></td>
          <td><%= dish_discount.desc %></td>
          <td><%= dish_discount.price %></td>
          <td><%= dish_discount.store %></td>
          <td><%= link_to 'Show', [@store, dish_discount] %></td>
          <td><%= link_to 'Edit', edit_store_dish_discount_path(@store, dish_discount) %></td>
          <td><%= link_to 'Destroy', [@store, dish_discount], method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
    <% end %>
  </table>

  <br />

  <%= link_to 'New Dish discount', new_store_dish_discount_path, :class => 'btn btn-primary' %>
  <%= link_to 'Back to Stores', stores_path, :class => 'btn btn-info' %>
</div>